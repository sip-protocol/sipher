# Server
NODE_ENV=development
PORT=5006
LOG_LEVEL=info

# Authentication
API_KEYS=dev-key-1,dev-key-2

# Solana
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# Proxy
TRUST_PROXY=1

# Graceful Shutdown
SHUTDOWN_TIMEOUT_MS=30000
